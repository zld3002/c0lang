void test_charat() {
  string a = "abba";
  assert(string_charat(a,0) == string_charat(a,3)); // "Char mismatch"
  assert(string_charat(a,0) < string_charat(a,1)); // "Char mismatch"
  assert(string_charat(a,1) > string_charat(a,0)); // "Char mismatch"
  assert(string_charat(a,0) != string_charat(a,1)); // "Char mismatch"
}

int main() {
  test_charat();
  return 0;
}
